{
	"name": "Chrolog - TabTracker",
	"version": "1.0",
	"manifest_version": 3,
	"description": "Retrive the activeTab and send it to the Chrolog server",
	"action": {
		"default_popup": "index.html"
	},
	"permissions": ["activeTab", "tabs"],
	"background": {
		"service_worker": "service-worker-loader.js",
		"type": "module"
	},
	"host_permissions": ["https://*/*", "http://*/*"],
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
}
